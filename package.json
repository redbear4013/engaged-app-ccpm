{
  "name": "engaged-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "prepare": "husky install || true",
    "db:setup": "node scripts/setup-database.js",
    "scraper:start": "dotenv -e .env.local -- node -r ts-node/register --project tsconfig.node.json src/workers/start-scraper.ts",
    "scraper:dev": "dotenv -e .env.local -- nodemon --exec 'ts-node --project tsconfig.node.json' src/workers/start-scraper.ts",
    "scraper:health": "dotenv -e .env.local -- node -r ts-node/register --project tsconfig.node.json src/workers/health-check.ts",
    "performance:audit": "node scripts/performance-audit.js",
    "performance:bundle": "node scripts/bundle-analyzer.js",
    "performance:lighthouse": "npm run build && npm run start & sleep 10 && npm run performance:audit && kill %1",
    "performance:analyze": "ANALYZE=true npm run build",
    "performance:ci": "npm run build && npm run performance:bundle && npm run performance:lighthouse",
    "build:production": "NODE_ENV=production npm run build",
    "start:production": "NODE_ENV=production npm run start",
    "deploy:check": "npm run type-check && npm run lint && npm run test && npm run build:production",
    "optimize:images": "next-optimized-images",
    "precommit": "npm run lint:fix && npm run format && npm run type-check"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-slot": "^1.2.3",
    "@stripe/stripe-js": "^7.9.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.4",
    "@tanstack/react-query": "^5.89.0",
    "@types/react-big-calendar": "^1.16.3",
    "bull": "^4.16.3",
    "cheerio": "^1.0.0",
    "chrono-node": "^2.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "fast-levenshtein": "^3.0.0",
    "framer-motion": "^12.23.21",
    "fuse.js": "^7.0.0",
    "ioredis": "^5.4.1",
    "jsdom": "^25.0.1",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "node-cron": "^3.0.3",
    "playwright": "^1.50.0",
    "react": "19.1.0",
    "react-big-calendar": "^1.19.4",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.63.0",
    "react-resizable": "^3.0.5",
    "stripe": "^18.5.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.11",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.5.4",
    "@tailwindcss/postcss": "^4",
    "@tanstack/react-query-devtools": "^5.89.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cheerio": "^0.22.35",
    "@types/fast-levenshtein": "^0.0.4",
    "@types/jest": "^30.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "chrome-launcher": "^1.2.1",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "lighthouse": "^12.8.2",
    "nodemon": "^3.1.7",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5"
  }
}
