    {
      "id": "eventbrite-macau",
      "name": "Eventbrite Macau",
      "baseUrl": "https://www.eventbrite.com/d/macau/events/",
      "sourceType": "website",
      "scrapeConfig": {
        "selectors": {
          "title": "[data-testid='event-title']",
          "description": "[data-testid='event-description']",
          "startTime": "[data-testid='event-datetime']",
          "location": "[data-testid='event-location']",
          "price": "[data-testid='event-price']",
          "image": "[data-testid='event-image'] img",
          "link": "[data-testid='event-link']"
        },
        "waitFor": {
          "selector": "[data-testid='event-title']",
          "timeout": 10000,
          "networkIdle": true
        },
        "pagination": {
          "enabled": true,
          "nextButtonSelector": "[data-testid='pagination-next']",
          "maxPages": 5
        },
        "userAgent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36",
        "viewport": {
          "width": 1280,
          "height": 720
        },
        "delay": 2000,
        "retries": 3
      },
      "scrapeFrequencyHours": 6,
      "isActive": true,
      "errorCount": 0
    },
